/* pages/baoming/baoming.wxss */
.container{
  border-top: 1rpx solid #f6f6f6;
  padding: 40rpx 30rpx;
  box-sizing: border-box;
}
.f_view{
  width: 690rpx;
  
}
.f_li{
  width: 100%;
  height: 100rpx;
  padding: 20rpx;
  box-sizing: border-box;
  background: #F6F6F6;
  border-radius: 4rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-bottom: 30rpx;
  font-size: 34rpx;
  color: #222;
}
.fli_name{
  width: 136rpx;
  text-align:justify;
  text-align-last:justify;
}
.fli_sx{
  width:1px;
  height:60rpx;
  background:rgba(209,209,209,1);
}
.fli_int{
  width: 400rpx;
}
.sub_btn:after{
  border: 0;
  background: transparent;
  color: #fff;
}
.sub_btn{
  background: #3879D1;
  color: #fff;
  /* height:94rpx; */
}
.other{
  width: 100%;
  padding: 0 20rpx;
  font-size:34rpx;
  color: #222;
  margin-bottom: 30rpx;
  box-sizing: border-box;
}
.f_area{
  width: 100%;
  height:208rpx;
  padding: 20rpx;
  box-sizing: border-box;
  background: #f6f6f6;
  margin-bottom: 30rpx;
  border-radius: 4rpx;
}
.yy_btn{
	position: relative;
	width: 100%;
	height: 100rpx;
	background: #3879D1;
	color: #fff;
	font-size: 36rpx;
	display: flex;
	align-items: center;
	justify-content: center;
}