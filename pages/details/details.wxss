/* pages/details/details.wxss */
.container{
	padding-bottom: 150rpx;
	box-sizing: border-box;
}
.xq_banner{
	width: 100%;
	height: 282rpx;
}
.xq_banner image{
	width: 100%;
	height:100%;
}
.xq_msg{
	width: 100%;
}
.xq_msg table,.xq_msg view,.xq_msg table.xq_msg text,.xq_msg video{
	max-width: 100%;
}
.yy_btn{
	position: fixed;
	bottom: 0;
	left: 0;
	width: 100%;
	height: 100rpx;
	background: #3879D1;
	color: #fff;
	font-size: 36rpx;
	display: flex;
	align-items: center;
	justify-content: center;
}